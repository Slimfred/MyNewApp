<p id="notice"><%= notice %></p>

<div class="user-form container-fluid col-sm-4 col-sm-offset-4 col-xs-10 col-xs-offset-1">
  <h3 class="user-labels">
    First name:
    <%= @user.first_name %>
  </h3>

  <h3 class="user-labels">
    Last name:
    <%= @user.last_name %>
  </h3>

  <h3 class="user-labels">
    Email:
    <%= @user.email %>
  </h3>
</div> <!-- end user-form -->

<div class="shared-links col-sm-12">
  <%= link_to 'Edit', edit_user_path(@user) %> |
  <%= link_to 'Back', users_path %>
</div>
